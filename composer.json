{
    "name": "lws/exportactions",
    "description": "This package exports data from an API in CSV,PDF Formats",
    "license": "MIT",
    "authors": [
        {
            "name": "Phaniraj K",
            "email": "phaniraj.k@ladybirdweb.com",
            "homepage": "https://github.com/Phaniraj35"
        }
    ],
    "require": {
        "ext-zip": "*",
        "illuminate/support": "~5",
        "guzzlehttp/guzzle": "^6.3",
        "soapbox/laravel-formatter": "^3.1",
        "elibyy/tcpdf-laravel": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~8",
        "orchestra/testbench": ">=3.8"
    },
    "autoload": {
        "psr-4": {
            "LWS\\ExportActions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LWS\\ExportActions\\Tests\\": "tests"
        }
    },
    "suggest": {
        "sensiolabs/security-checker": "Required to use the security check command"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LWS\\ExportActions\\ExportServiceProvider",
                "LWS\\ExportActions\\CustomEventServiceProvider"
            ]
        }
    }
}
